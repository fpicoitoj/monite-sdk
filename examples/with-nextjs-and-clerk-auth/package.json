{
  "name": "sdk-demo-with-nextjs-and-clerk-auth",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "apiVersion": "2024-05-25",
  "scripts": {
    "dev": "next dev --port 3000",
    "build": "next build",
    "build:clean": "rm -rf .next && rm -rf node_modules/.cache && next build",
    "build:production": "rm -rf .next && rm -rf node_modules/.cache && NODE_ENV=production BUILD_ID=$(git rev-parse --short HEAD 2>/dev/null || date +%s) next build",
    "start": "next start --port 3000",
    "lint": "next lint",
    "test": "jest",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "typecheck": "tsc --noEmit",
    "extract-translations": "lingui extract --config lingui.config.cjs",
    "compile-translations": "lingui compile --config lingui.config.cjs",
    "download-openapi-document": "yarn exec ./download-openapi-document.sh",
    "codegen": "yarn codegen:api-version && yarn exec ./generate-api-client.sh",
    "codegen:api-version": "echo \"// This file was generated automatically\nexport const apiVersion = '$(node -p 'require(\"@monite/sdk-react/package.json\").apiVersion')';\" > src/lib/monite-api/api-version.ts"
  },
  "dependencies": {
    "@clerk/clerk-sdk-node": "5.1.6",
    "@clerk/nextjs": "6.11.1",
    "@emotion/cache": "~11.11.0",
    "@lingui/core": "~4.7.0",
    "@lingui/macro": "~4.7.0",
    "@lingui/react": "~4.7.0",
    "@monite/sdk-react": "workspace:~",
    "@mui/icons-material": "~5.14.3",
    "@mui/material": "~5.15.14",
    "@mui/material-nextjs": "^5.15.11",
    "@radix-ui/react-select": "~2.2.5",
    "@tanstack/react-query": "~5.28.6",
    "@team-monite/sdk-demo": "workspace:~",
    "@types/node": "20.3.1",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "aedes": "~0.50.1",
    "buffer": "~6.0.3",
    "commander": "~11.1.0",
    "date-fns": "~4.1.0",
    "eslint": "8.57.0",
    "eslint-config-next": "~14.0.3",
    "lucide-react": "~0.514.0",
    "mqtt": "~5.3.1",
    "next": "~14.2.26",
    "openapi-fetch": "~0.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-use": "~17.6.0",
    "recharts": "~2.15.3",
    "svix": "~1.13.0",
    "typescript": "~5.5.4"
  },
  "devDependencies": {
    "@clerk/testing": "^1.7.1",
    "@faker-js/faker": "~8.3.1",
    "@lingui/cli": "~4.7.0",
    "@lingui/swc-plugin": "4.0.8",
    "@openapi-qraft/cli": "1.14.0",
    "@playwright/test": "^1.52.0",
    "@tailwindcss/postcss": "~4.1.11",
    "@testing-library/jest-dom": "~6.1.4",
    "@testing-library/react": "~14.1.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/babel-plugin-macros": "^3",
    "autoprefixer": "~10.4.21",
    "babel-plugin-macros": "~3.1.0",
    "chalk": "~5.3.0",
    "dotenv": "^16.5.0",
    "dotenv-cli": "^7.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "glob": "~11.0.3",
    "jest": "~29.7.0",
    "jest-environment-jsdom": "~29.7.0",
    "postcss": "~8.5.6",
    "postcss-flexbugs-fixes": "~5.0.2",
    "postcss-import": "~16.1.1",
    "postcss-preset-env": "~10.2.4",
    "prettier": "^2.8.8",
    "smee-client": "~2.0.0",
    "tailwindcss": "~4.1.11",
    "terser-webpack-plugin": "~5.3.14",
    "tsup": "~8.0.1",
    "webpack": "~5.99.9"
  },
  "packageManager": "yarn@3.6.4"
}
